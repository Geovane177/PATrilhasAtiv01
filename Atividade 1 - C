using System;

namespace Atividade_1___C
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Digite um número de ponto flutuante: ");
			
			
			float flutuante = float.Parse(Console.ReadLine());
			
			Console.WriteLine("O Número digitado é: "+ flutuante);
			
			
			Console.Write("Pressione para continuar . . . ");
			Console.ReadKey(true);
		}
	}
}
